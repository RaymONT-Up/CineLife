    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/axios": "^0.14.0",
    "@types/swiper": "^6.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.26",
